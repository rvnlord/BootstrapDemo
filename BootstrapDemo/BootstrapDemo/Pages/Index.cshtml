<!-- Modal -->
<div class="modal fade" tabindex="-1" id="modalLogin" data-keyboard="false" data-backdrop="static">
    <div class="modal-dialog">

        <div class="modal-content">
            <div class="modal-header d-flex align-items-center">
                <h4 class="modal-title">Login</h4>
                <button type="button" class="close ml-auto align-self-start" data-dismiss="modal">
                    <i class="fa fa-times"></i>
                </button>
            </div>
            <div class="modal-body p-0">
                <form class="form">
                    <div class="container p-5px">
                        <div class="row">
                            <div class="col-lg-3 col-md-4 col-12">
                                <label for="inputUserName" class="col-form-label">Name</label>
                            </div>
                            <div class="col-lg-6 col-md-8 col-12">
                                <input class="form-control" type="text" id="inputUserName" placeholder="Username..." />
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-lg-3 col-md-4 col-12">
                                <label for="inputPassword" class="col-form-label">Password</label>
                            </div>
                            <div class="col-lg-6 col-md-8 col-12">
                                <input class="form-control" type="text" id="inputPassword" placeholder="Password..." />
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer p-0">
                <div class="container p-5px">
                    <div class="row">
                        <div class="offset-lg-3 col-lg-3 offset-md-4 col-md-4 col-12">
                            <button class="btn btn-primary w-100">Login</button>
                        </div>
                        <div class="col-lg-3 col-md-4 col-12">
                            <button id="btnHideModal" type="button" class="btn btn-primary w-100">Close</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
</div>

